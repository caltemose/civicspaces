extends ../layout

block title
  title #{sitetitle} | Find Spaces Using a Map

block content

  div.container
  
    h1 Available Spaces

    div#googlemap(style="height:400px")
    

block scripts
  script(src='https://maps.googleapis.com/maps/api/js?sensor=false')
  script(src="/assets/js/cs.map.js")
  script.
    $(document).ready(function(){
      cs.map.init('#googlemap', 33.7811643, -84.38362970000003);
      cs.map.initMultiple();
    })